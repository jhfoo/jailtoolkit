{
  "name": "dhcp-golemm",
  "release": "12.2-RELEASE",
  "props": {
    "vnet": true,
    "ip4_addr": "vnet0|192.168.86.3/24",
    "defaultrouter": "192.168.86.1",
    "resolver": "search golem.kungfoo.info;nameserver 192.168.88.5",
    "dhcp": false,
    "bpf": true,
    "boot": true,
    "template": false,
    "allow_raw_sockets": true,
    "vnet_default_interface": "jailvlan"
  },
  "pkgs": [
    "zsh"
  ],
  "tasks": [
    {
      "name": "install dnsmasq",
      "do": "runtemplate",
      "template": "dnsmasq"
    },
    {
      "name": "install consul",
      "do": "runtemplate",
      "template": "consul"
    },
    {
      "name": "restart jail",
      "do": "jailrestart"
    }
  ]
}